auto lo

# Loop
iface lo inet loopback

# Wired
iface eth0 inet static
#  address   192.168.12.60
#  broadcast 192.168.12.255
#  gateway   192.168.12.144

  address   192.168.2.60
  broadcast 192.168.2.255
  gateway   192.168.2.1


# WiFi
auto wlan0
iface wlan0 inet static
  address   192.168.12.65
  broadcast 192.168.12.255
  gateway   192.168.12.144
  wpa-ssid  foosball
  wpa-psk   528a501049b912f163f4b423ee983868ef865a7e27bd516b14cd85ead5e82b2f


#  address   192.168.2.65
#  broadcast 192.168.2.255
#  gateway   192.168.2.1
#  wpa-ssid  google-access-point
#  wpa-psk   2d5f23ce938856412e206db92bc166016ffdff8a00d373fdeb571fc149f65fc3

# Default
iface default inet dhcp
